{
	"name": "potree",
	"version": "1.6.0",
	"description": "potree wrapper for threejs applications",
	"main": "source/Main.js",
	"files": "",
	"scripts": {
		"build": "rollup -c",
		"build-min": "rollup -c && npm run uglify",
		"docs": "jsdoc -d docs source",
		"uglify": "uglifyjs --compress --mangle --output build/potree.min.js -- build/potree.js",
		"pub": "npm run build && npm run docs && npm publish --access public .",
		"start": "python -m http.server"
	},
	"keywords": [
		"three",
		"potree",
		"3d",
		"webgl"
	],
	"author": "Tentone",
	"license": "MIT",
	"dependencies": {},
	"devDependencies": {
		"jsdoc": "^3.5.0",
		"rollup": "^1.0.0",
		"rollup-plugin-strip": "^1.2.1",
		"uglify-js": "^3.4.9"
	}
}
